function add(a,b){
    return a+b;

}
//fatarrow_function
const fatarrow=(a,b)=>a+b;

const result1=add(3,4);
const result2=fatarrow(3,4);
console.log(result1,result2);

//object 

const student={
    name:'anjali',
    age:20,
    greet:function (){console.log("Hii,This is ",this.name)}
};
student.greet();
